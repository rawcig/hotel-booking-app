{
  "expo": {
    "name": "Hotel Booking",
    "slug": "bookingApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/SETEC-Logo.png",
    "scheme": "bookingapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icons/SETEC-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#dedede",
      "textColor": "#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/SETEC-Logo.png",
        "backgroundColor": "#dedede"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.rawh.bookingApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/SETEC-Logo.png"
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
